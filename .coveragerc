[run]
branch = True
omit =
    */migrations/*
    */south_migrations/*
    smart_manager/version.py
    smart_manager/apps.py
source = smart_manager
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1
